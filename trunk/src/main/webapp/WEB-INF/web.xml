<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
    <servlet-name>prueba</servlet-name>
    <servlet-class>com.canchita.web.PruebaServlet</servlet-class>
  </servlet>
    <servlet>
    	<servlet-name>lala</servlet-name>
    	<display-name>lala</display-name>
    	<description></description>
    	<servlet-class>
    	lala</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>ListComplex</servlet-name>
    	<display-name>ListComplex</display-name>
    	<description></description>
    	<servlet-class>
    	com.canchita.controller.complex.ListComplex</servlet-class>
    </servlet>
  <servlet-mapping>
    <servlet-name>prueba</servlet-name>
    <url-pattern>/prueba</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>lala</servlet-name>
  	<url-pattern>/lala</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListComplex</servlet-name>
  	<url-pattern>/ListComplex</url-pattern>
  </servlet-mapping>
  
</web-app>
